#
# ~/.bash_profile
#

[[ -f ~/.bashrc ]] && . ~/.bashrc

export GTK2_RC_FILES="/home/ok/.gtkrc-2.0"
export MOZ_DISABLE_PANGO=1
export OPERAPLUGINWRAPPER_PRIORITY=0
export OPERA_KEEP_BLOCKED_PLUGIN=1

export EDITOR='vim'
export VIEWER='vim -R'
export BROWSER='/home/ok/bin/browser'

export PATH="/usr/lib/cw:$PATH:/home/ok/bin/:/mnt/SD/texlive/bin/i386-linux/:/mnt/SD/eagle/bin:/mnt/SD/.WorldOfGoo/:/mnt/SD/.AngryBirds/:/mnt/SD/.Osmos/"

export NOCOLOR_PIPE=1
