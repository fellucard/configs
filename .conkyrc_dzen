out_to_x no
out_to_console yes
update_interval 1.0
if_up_strictness address

TEXT
 ^i(/home/ok/.dzen/icons/cpu.xbm) ^fg(\#A270A3)${cpu}% \
^fg()^i(/home/ok/.dzen/icons/temp.xbm) ^fg(\#8A2F58)${execpi 60 /home/ok/Scripts/temp.sh}Â° \
^fg()^i(/home/ok/.dzen/icons/mem.xbm) ^fg(\#6096BF)${memperc}% \
^fg()^i(/home/ok/.dzen/icons/bat_full_01.xbm) ^fg(\#914E89)${battery_short} \
^fg()^i(/home/ok/.dzen/icons/net_wired.xbm) ${if_up eth0}^fg(\#53A6A6)${downspeedf eth0} ^i(/home/ok/.dzen/icons/net_down_03.xbm)^fg(\#5E468C)^i(/home/ok/.dzen/icons/net_up_03.xbm) ${upspeedf eth0}${else}N/A${endif} \
^fg()^i(/home/ok/.dzen/icons/clock.xbm) ${time %a %d} ^fg(\#E0E0E0)${time %H:%M}            
