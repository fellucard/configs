!-- colors --!
#include "/home/ok/.colors/dc3"

!-- urxvt --!
urxvt.scrollBar:          false
urxvt.font:               -ok100-tamsyn-medium-*-*-*-11-*-*-*-*-*-*-*
urxvt.boldFont:           -ok100-tamsyn-medium-*-*-*-11-*-*-*-*-*-*-*
urxvt.borderLess:         false
urxvt.buffered:           true
urxvt.urgentOnBell:       true
urxvt.geometry:           81x21
urxvt.cursorBlink:        true
URxvt.perl-ext-common:    default,matcher
URxvt.urlLauncher:        opera-next
URxvt.matcher.button:     1
URxvt.matcher.pattern.1:  \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

!-- dzen2 --!
dzen2.font:               -*-glisp-medium-*-*-*-11-*-*-*-*-*-*-*
!dzen2.font:               -ok100-tamsyn-medium-*-*-*-11-*-*-*-*-*-*-*
dzen2.foreground:         #44484C
dzen2.background:         #0D131A
