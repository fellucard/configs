!-- colors --!
#include "/home/ok/.colors/dark"
!#include "/home/ok/.colors/matrix"
!#include "/home/ok/.colors/phrakture"
!#include "/home/ok/.colors/zenburn"

!-- urxvt --!
#urxvt*cursorBlink:        true
urxvt*scrollBar:          false
urxvt*background:         #121212
urxvt*foreground:         #999999
urxvt*cursorColor:        #5E468C
urxvt*font:               xft:terminus:medium:size=9
urxvt*boldFont:           xft:terminus:medium:size=9
urxvt*borderLess:         false
urxvt*buffered:           true
urxvt*urgentOnBell:       true
urxvt*geometry:           80x22

!urxvt*depth:      32
!urxvt*background: rgba:3f00/3f00/3f00/dddd
!If that color does not work properly, try:
!!urxvt*background: rgba:1111/1111/1111/dddd
