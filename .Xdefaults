!-- colors --!
#include "/home/ok/.colors/dark"

!-- urxvt --!
urxvt*scrollBar:          false
urxvt*background:         #121212
urxvt*foreground:         #999999
urxvt*cursorColor:        #5E468C
#urxvt*font:               -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
#urxvt*boldFont:           -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
urxvt*font:               xft:droid sans mono dotted:medium:size=9
urxvt*boldFont:           xft:droid sans mono dotted:bold:size=9
#urxvt*font:               xft:envy code r:medium:size=9
#urxvt*boldFont:           xft:envy code r:medium:size=9
#urxvt*font:               xft:inconsolata:medium:size=8
#urxvt*boldFont:           xft:inconsolata:bold:size=8
urxvt*borderLess:         false
urxvt*buffered:           true
urxvt*urgentOnBell:       true
urxvt*geometry:           80x22
#urxvt*cursorBlink:        true

