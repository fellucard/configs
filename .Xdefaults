!-- colors --!
#include "/home/ok/.colors/dc2"

!-- urxvt --!
urxvt.scrollBar:          false
urxvt.font:               -ok100-tamsyn-medium-*-*-*-11-*-*-*-*-*-*-*
urxvt.boldFont:           -ok100-tamsyn-medium-*-*-*-11-*-*-*-*-*-*-*
urxvt.borderLess:         false
urxvt.internalBorder:     1
urxvt.buffered:           true
urxvt.urgentOnBell:       true
urxvt.geometry:           81x21
urxvt.cursorBlink:        true
URxvt.perl-ext-common:    default,matcher
URxvt.urlLauncher:        /home/ok/bin/browser
URxvt.matcher.button:     1
URxvt.matcher.pattern.1:  \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

!-- dzen2 --!
dzen2.font:               -*-termsyn.icons-medium-*-*-*-11-*-*-*-*-*-*-*
dzen2.foreground:         #999999
dzen2.background:         #020202
